//If items with a white (common) item name should get a beam
showWhiteItems=false

//How many particles to create per tick
particleCount=1

//How tall the loot beam is (in blocks)
beamHeight=0.8

//How much to offset the beam vertically (in blocks)
beamOffset=0.2

//How old the item needs to be before it gets a beam (in ticks)
minimumAge=10

//If the mod should print caught errors instead of swallowing them
printErrors=false

