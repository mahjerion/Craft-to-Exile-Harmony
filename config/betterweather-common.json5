{ 
	"acid_rain": { 
		"world": { 
			/* 
			   The chance of acid rain that's checked every 24,000 ticks(1 Minecraft day).
			   Default: 0.25
			*/
			"acidRainChance": 0.01,
			/* 
			   How often(in ticks) blocks are destroyed during an acid rain event.
			   Default: 0.25
			*/
			"blockTickDestroySpeed": 300,
			/* 
			   Destroy Blocks?
			   Default: true
			*/
			"destroyBlocks": true,
			/* 
			   Destroy what block materials? Allowed Values: GRASS,LEAVES,PLANTS,CROPS
			   Default: GRASS,LEAVES,PLANTS,CROPS
			*/
			"allowedBlocksToDestroy": "GRASS,LEAVES,PLANTS,CROPS",
			/* 
			   A specific block in one of the materials you want to destroy? Blacklist them here w/ their registry names!
			   I.E: "minecraft:rose,minecraft:wither_rose"
			*/
			"blocksToNotDestroy": "",
			/* 
			   The block to change grass to.
			   Default: "minecraft:dirt"
			*/
			"blockToChangeFromGrass": "minecraft:dirt"
		},
		"entity": { 
			/* 
			   Does Acid Rain hurt entities(including players)?
			   Default: true
			*/
			"hurtEntities": true,
			/* 
			   How often(in ticks) are entities(including players) hurt?
			   Default: 150
			*/
			"entityDamageTickSpeed": 120,
			/* 
			   What types of entities are damaged by Acid Rain?
			   Default: MONSTER,PLAYER
			*/
			"entityTypesToDamage": "MONSTER,PLAYER",
			/* 
			   The amount of damage taken.
			   Default is 0.5
			*/
			"damageStrength": 0.5
		},
		"client": { 
			/* 
			   Acid Rain Smoke Particles?
			   Default: true
			*/
			"smokeParticles": false
		}
	},
	"blizzard": { 
		"world": { 
			"snow_generation": { 
				/* 
				   The chance of a blizzard that's checked every 24,000 ticks(1 Minecraft day).
				   Default: 0.1
				*/
				"blizzardChance": 0.01,
				/* 
				   Do blizzards occur in deserts?
				   Default: false
				*/
				"blizzardsInDeserts": false,
				/* 
				   Do blizzards generate snow and ice?
				   Default: true
				*/
				"spawnSnowAndIce": true,
				/* 
				   How often(in ticks) is snow and ice placed during blizzards?
				   Default: 15
				*/
				"tickSnowAndIcePlaceSpeed": 200,
				/* 
				   Do blizzards destroy plants?
				   Default: false
				*/
				"destroyPlants": false
			},
			"snow_decay": { 
				/* 
				   Does snow and ice decay after blizzards in biomes within the specified temperature threshold?
